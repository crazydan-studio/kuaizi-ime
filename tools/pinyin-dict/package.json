    "got": "^13.0.0",
    "grapheme-splitter": "^1.0.4",
    "jsdom": "^22.1.0",
    "pinyin-pro": "^3.25.0",
    "sqlite": "^5.0.1",
    "sqlite3": "^5.1.6"
