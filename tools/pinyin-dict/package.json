{
  "name": "pinyin-dict",
  "version": "0.1.0",
  "description": "Pinyin dictionary maker",
  "scripts": {
    "web": "node src/web.mjs",
    "generate:test": "node src/generate/test.mjs",
    "generate:raw": "node src/generate/raw/index.mjs",
    "generate:raw:debug": "node --inspect-brk src/generate/raw/index.mjs",
    "generate:sqlite": "node src/generate/sqlite/all/index.mjs",
    "generate:sqlite:diff": "node src/generate/sqlite/all/diff.mjs",
    "generate:sqlite:ime": "node src/generate/sqlite/ime/index.mjs",
    "generate:sqlite:debug": "node --inspect-brk src/generate/sqlite/all/index.mjs",
    "generate:sqlite:phrase": "node src/generate/sqlite/phrase/index.mjs",
    "generate:sqlite:phrase:debug": "node --inspect-brk src/generate/sqlite/phrase/index.mjs",
    "generate:emoji": "node src/generate/emoji/index.mjs",
    "phrase:shell": "node src/generate/sqlite/phrase/shell.mjs",
    "phrase:shell:debug": "node --inspect-brk src/generate/sqlite/phrase/shell.mjs"
