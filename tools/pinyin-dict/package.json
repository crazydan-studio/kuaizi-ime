    "generate:sqlite:ime": "node src/generate/sqlite/ime/index.mjs",
    "generate:sqlite:word": "node src/generate/sqlite/word/index.mjs",
    "generate:sqlite:word:debug": "node --inspect-brk src/generate/sqlite/word/index.mjs",
    "generate:sqlite:word:diff": "node src/generate/sqlite/word/diff.mjs",
    "generate:sqlite:phrase:hmm": "node src/generate/sqlite/phrase/hmm/index.mjs",
